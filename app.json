{
  "expo": {
    "name": "Just-Pub!",
    "slug": "Just-Pub",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/just-pub.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "uk.co.jdcook.justpub",
      "supportsTablet": true
    },
    "android": {
      "package": "uk.co.jdcoperations.justpub",
      "permissions": [
        "CAMERA"
      ]
    }
  }
}
